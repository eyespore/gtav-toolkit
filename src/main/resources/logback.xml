<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>
<!--                %d{yyyy-MM-dd HH:mm:ss} [%p][%c][%M][%L] -> %m%n-->
                %red(%date{yyyy-MM-dd HH:mm:ss}) %highlight(%-5level) %red([%thread]) %boldMagenta(%logger{50}[%L]) %cyan(%msg%n)
            </pattern>
        </encoder>
    </appender>

    <!-- 默认输出 -->
    <root level="debug">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
